# YourVoice Frontend - Environment Configuration
# Copy this file to .env.local and fill in the values

# Backend API URL
# For local development: http://localhost:8000
# For OSC deployment: https://your-backend-instance.eyevinn-web-runner.auto.prod.osaas.io
VITE_BACKEND_URL=http://localhost:8000

# Backend API Version (optional, defaults to api/v1/)
VITE_BACKEND_API_VERSION=api/v1/

# Backend API Key (optional, for protected endpoints)
VITE_BACKEND_API_KEY=

# Debug Mode (optional)
VITE_DEBUG_MODE=false

# Dev Logger Level (optional, 0-3)
VITE_DEV_LOGGER_LEVEL=3
